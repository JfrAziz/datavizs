{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "src",
    "paths": {
      "@app/*": ["app/*"],
      "@lib/*": ["lib/*"],
      "@assets": ["assets/"],
      "@utils/*": ["utils/*"],
      "@pages/*": ["pages/*"],
      "@assets/*": ["assets/*"],
      "@layouts/*": ["layouts/*"],
      "@components/*": ["components/*"]
    },
  }
}
